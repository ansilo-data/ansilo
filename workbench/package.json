{
  "name": "workbench",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "sh -c 'NODE_INSTANCE=nodes/${0:-contacts} node_modules/.bin/next dev -p ${1:-5000}'",
    "build": "next build",
    "start": "sh -c 'NODE_INSTANCE=nodes/${0:-contacts} node_modules/.bin/next start -p ${1:-5000}'",
    "start:all": "sh -c \"ls nodes | tr '\\t' '\\n' | cat -n | awk '{print \\\"\\042 npm run start \\\" \\$2 \\\" \\\" 500\\$1 \\\"\\042\\\"}' | xargs node_modules/.bin/concurrently\"",
    "lint": "next lint"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.2",
    "@mui/lab": "^5.0.0-alpha.81",
    "@mui/material": "^5.7.0",
    "@reduxjs/toolkit": "^1.8.1",
    "@types/d3": "^7.4.0",
    "d3": "^7.4.4",
    "lodash": "^4.17.21",
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-redux": "^8.0.1",
    "recharts": "^2.1.9",
    "sql.js": "^1.6.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@types/node": "17.0.32",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.3",
    "@types/sql.js": "^1.4.3",
    "concurrently": "^7.2.1",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "typescript": "4.6.4"
  }
}
