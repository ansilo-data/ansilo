{
  "id": "n-teradata",
  "name": "Teradata",
  "url": "TODO",
  "icon": "icons/teradata.svg",
  "tags": [],
  "schema": {
    "entities": [
      {
        "id": "e-teradata-contacts",
        "name": "Contacts",
        "description": "The list of entities (individuals or businesses) which interact with the organisation",
        "tags": [{ "key": "t-vertical", "value": "Marketing" }],
        "versions": [
          {
            "id": "e-teradata-contacts-v-1.0",
            "version": { "major": 1, "minor": 0, "patch": 0 },
            "attributes": [
              {
                "id": "e-teradata-contacts-a-id",
                "name": "id",
                "description": "The primary key of the contact",
                "type": { "name": "INT" }
              },
              {
                "id": "e-teradata-contacts-a-type",
                "name": "type",
                "description": "Discriminates whether the contact is an individual or legal entity",
                "type": { "name": "VARCHAR(10)" }
              },
              {
                "id": "e-teradata-contacts-a-name",
                "name": "name",
                "description": "This is the legal name of the individual or business",
                "type": { "name": "VARCHAR(255)" }
              },
              {
                "id": "e-teradata-contacts-a-registered",
                "name": "registered",
                "description": "The timestamp which the contact became a client of our business",
                "type": { "name": "TIMESTAMP" }
              }
            ]
          },
          {
            "id": "e-teradata-contacts-v-1.1",
            "version": { "major": 1, "minor": 1, "patch": 0 },
            "attributes": [
              {
                "id": "e-teradata-contacts-a-id",
                "name": "id",
                "description": "The primary key of the contact",
                "type": { "name": "INT" }
              },
              {
                "id": "e-teradata-contacts-a-type",
                "name": "type",
                "description": "Discriminates whether the contact is an individual or legal entity",
                "type": { "name": "VARCHAR(10)" }
              },
              {
                "id": "e-teradata-contacts-a-name",
                "name": "name",
                "description": "This is the legal name of the individual or business",
                "type": { "name": "VARCHAR(255)" }
              },
              {
                "id": "e-teradata-contacts-a-dob",
                "name": "date_of_birth",
                "description": "For individual teradata, this is the legal date of birth of the individual",
                "type": { "name": "DATE" }
              },
              {
                "id": "e-teradata-contacts-a-registered",
                "name": "registered",
                "description": "The timestamp at which the contact became a client of our business",
                "type": { "name": "TIMESTAMP" }
              }
            ]
          }
        ]
      },
      {
        "id": "e-teradata-interactions",
        "name": "Interactions",
        "description": "The list of recorded interactions teradata have had with our business across digital touch points.",
        "tags": [{ "key": "t-vertical", "value": "Marketing" }],
        "versions": [
          {
            "id": "e-teradata-interactions-v-1.0",
            "version": { "major": 1, "minor": 0, "patch": 0 },
            "attributes": [
              {
                "id": "e-teradata-interactions-a-id",
                "name": "id",
                "description": "The primary key of the interaction",
                "type": { "name": "INT" }
              },
              {
                "id": "e-teradata-interactions-a-contact",
                "name": "contact_id",
                "description": "The ID of the contact who was responsible for this interaction",
                "type": { "name": "INT" }
              },
              {
                "id": "e-teradata-interactions-a-type",
                "name": "type",
                "description": "The type of interaction against this contact",
                "type": { "name": "VARCHAR(10)" }
              },
              {
                "id": "e-teradata-interactions-a-recorded",
                "name": "recorded",
                "description": "The time at which this interaction was recorded",
                "type": { "name": "TIMESTAMP" }
              }
            ]
          }
        ]
      }
    ]
  },
  "jobs": [
    {
      "id": "j-teradata-import-contacts",
      "name": "Import contacts",
      "description": "Retrieves the latest batch of contact records",
      "query": {
        "sql": "SELECT * from contacts.contacts_v1_0"
      },
      "destination": {
        "type": "table",
        "options": {}
      },
      "trigger": { "type": "manual" },
      "runs": [{ "status": "success", "recordsCount": 200 }]
    },
    {
      "id": "j-teradata-import-accounts",
      "name": "Import accounts",
      "description": "Merges the latest account records",
      "query": {
        "sql": "SELECT a.* FROM accounts.accounts_v1_0 ...."
      },
      "destination": {
        "type": "table",
        "options": {}
      },
      "trigger": { "type": "manual" },
      "runs": [{ "status": "success", "recordsCount": 100 }]
    },
    {
      "id": "j-teradata-import-transactions",
      "name": "Import transactions",
      "description": "Merges the latest transaction records",
      "query": {
        "sql": "SELECT a.* FROM accounts.transactions_v1_0 ...."
      },
      "destination": {
        "type": "table",
        "options": {}
      },
      "trigger": { "type": "manual" },
      "runs": [
        { "status": "success", "recordsCount": 300 },
        { "status": "error" }
      ]
    },
    {
      "id": "j-teradata-import-sf",
      "name": "Import Salesforce data",
      "description": "Retrieves the latest customer data from Salesforce",
      "query": {
        "sql": "SELECT a.* FROM salesforce_crm.contacts_v1_0 ...."
      },
      "destination": {
        "type": "table",
        "options": {}
      },
      "trigger": { "type": "manual" },
      "runs": [
        { "status": "success", "recordsCount": 500 }
      ]
    }
  ]
}
