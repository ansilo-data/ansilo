{
  "containerDefinitions": [
    {
      "name": "devcontainer",
      "image": "635198228996.dkr.ecr.ap-southeast-2.amazonaws.com/devcontainer:latest",
      "cpu": 0,
      "links": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "devcontainers",
          "awslogs-region": "ap-southeast-2",
          "awslogs-create-group": "true",
          "awslogs-stream-prefix": "{{USER}}"
        }
      },
      "portMappings": [
        {
          "containerPort": 22,
          "hostPort": 22,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "entryPoint": ["/bin/bash"],
      "command": ["/boot.sh", "{{USER}}"],
      "linuxParameters": {
        "capabilities": { "add": ["SYS_PTRACE"] }
      },
      "environment": [],
      "mountPoints": [
        {
          "sourceVolume": "dev-volume",
          "containerPath": "/efs",
          "readOnly": false
        }
      ],
      "volumesFrom": [],
      "dnsServers": [],
      "dnsSearchDomains": [],
      "dockerSecurityOptions": [],
      "systemControls": []
    }
  ],
  "family": "{{USER}}-devcontainer",
  "executionRoleArn": "arn:aws:iam::635198228996:role/ECSTaskExecutionRoleForPullingECR",
  "taskRoleArn": "arn:aws:iam::635198228996:role/DevContainerTaskRole",
  "networkMode": "awsvpc",
  "volumes": [
    {
      "name": "dev-volume",
      "efsVolumeConfiguration": {
        "fileSystemId": "fs-0b56141023f21f062",
        "rootDirectory": "/",
        "transitEncryption": "DISABLED",
        "authorizationConfig": {
          "iam": "DISABLED"
        }
      }
    }
  ],
  "placementConstraints": [],
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "4096",
  "memory": "8192",
  "tags": [
    {
      "key": "ecs:taskDefinition:createdFrom",
      "value": "ecs-console-v2"
    }
  ]
}
